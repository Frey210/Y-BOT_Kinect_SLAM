<launch>
    <!-- Menjalankan node robot_odom_node.py
    <node name="robot_odom_node" pkg="my_robot_pkg" type="robot_odom_node.py" output="screen"/> -->

    <!-- Menjalankan node odom_fusion_node.py -->
    <!-- <node name="odom_fusion_node" pkg="my_robot_pkg" type="odom_fusion_node.py" output="screen"/> -->

    <!-- Menjalankan node odom_fusion_node.py -->
    <!-- <node name="arduino_handler_node" pkg="my_robot_pkg" type="arduino_handler_rosserial.py" output="screen"/> -->

    <!-- Menjalankan node odom_fusion_node.py -->
    <node name="x360_joy_node" pkg="my_robot_pkg" type="x360_joy_node.py" output="screen"/>

    <!-- Menjalankan node odom_fusion_node.py -->
    <node name="joy_node" pkg="joy" type="joy_node" output="screen"/>

    <!-- Menjalankan node serial_node.py -->
    <node pkg="rosserial_python" type="serial_node.py" name="motor_serial_node">
      <param name="port" value="/dev/ttyACM0"/>
      <param name="baud" value="115200"/>
    </node>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
      <param name="port" value="/dev/ttyUSB1"/>
      <param name="baud" value="115200"/>
    </node>
</launch>
